{
    "name": "shelton/firebase-provider",
    "description": "Firebase Service Provider for Lumen/Laravel",
    "license": "MIT",
    "keywords": ["lumen", "firebase"],
    "type": "library",
    "require": {
        "php": "^7.4",
        "google/cloud-firestore": "^1.12",
        "kreait/laravel-firebase": "^2.3"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Shelton\\Firebase\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shelton\\Firebase\\Providers\\ServiceProvider"
            ]
        }
    }
}
